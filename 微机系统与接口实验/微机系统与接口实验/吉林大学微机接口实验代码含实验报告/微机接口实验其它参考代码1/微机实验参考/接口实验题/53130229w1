SSTACK SEGMENT STACK
       DW 32 DUP(?)
SSTACK ENDS
CODE   SEGMENT
	   ASSUME CS:CODE
START: MOV DX,0646H        
	   MOV AL,90H     
	   OUT DX,AL
AA1:   MOV DX,0640H      
       IN AL,DX
       CMP AL,00FFH
       JZ AA3
       
       MOV DX,0642H       
       OUT DX,AL
       
       MOV BL,AL
       AND BL,0001H
       CMP  BL,01H
       JZ AA4
       CMP  BL,00H
       JZ AA5
      
       
       JMP AA1

AA4:   MOV DX,0642H
       MOV AX,00F0H
       OUT DX,AX
       JMP AA1
AA5:   MOV DX,0642H
       MOV AL,0FH
       OUT DX,AX
       JMP AA1
AA3:   
CODE   ENDS
       END START
       
	